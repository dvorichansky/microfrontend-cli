{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "types": ["vue"],
        "baseUrl": ".",
        "noCheck": true,
        "paths": {
            "@/*": ["src/*"],
            "@mf-types/*": ["@mf-types/*"]
        }
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
